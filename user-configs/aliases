# ala - Alias Add - Add new aliases to this file
ala(){ echo "alias $1='$2'" | tee -a "$HOME/user-configs/aliases"; source "$HOME/user-configs/aliases"}

alias aliasadd='ala'
### AUTOMATICALLY GENERATED BY aliasadd() ###

alias ssh='zssh'
alias chez='chezmoi'
alias air='~/go/bin/air'

