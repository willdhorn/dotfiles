syntax on
filetype on
set number
set cursorline

" shiftwidth=4 tabstop=4 expandtab
set sw=2 sts=2 et

" line number column
hi LineNr ctermbg=black
hi LineNr ctermfg=white
hi SignColumn ctermbg=black
" line number color for current line
hi CursorLineNr ctermfg=red
" make comments dark
hi comment ctermfg=black

" keymaps
:nnoremap <S-u> <C-r> "Redo with SFT-U
 
